{
  "name": "leetcode-dark-theme-extension",
  "version": "1.0",
  "description": "let leetcode problem page look like dark theme",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "icons": {
    "16": "images/output1.png",
    "32": "images/output2.png",
    "48": "images/output3.png",
    "128": "images/output4.png"
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://leetcode.com/problems/*"
      ],
      "js": [
        "execute.js"
      ]
    }
  ]
}